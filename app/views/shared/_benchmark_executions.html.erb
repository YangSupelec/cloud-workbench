<%= will_paginate @benchmark_executions %>
<div id='defaultTable' class='body'>
  <table class='table responsive-table'>
    <thead>
    <tr>
      <th>Link</th>
      <% if show_benchmark_definition %>
      <th>Benchmark Definition</th>
      <% end %>
      <th>Status</th>
      <th>Started</th>
      <th>Ended</th>
      <th>Duration</th>
    </tr>
    </thead>
    <tbody>
    <% @benchmark_executions.each do |benchmark_execution| %>
      <tr>
        <td>
          <%= link_to benchmark_execution do %>
            <i class='fa fa-link'></i>
          <% end %>
        </td>
        <% if show_benchmark_definition %>
        <td>
          <%= link_to benchmark_execution.benchmark_definition.name, benchmark_execution.benchmark_definition %>
        </td>
        <% end %>
        <td>
          <%= benchmark_execution.status %>
        </td>
        <td>
          <%= benchmark_execution.start_time.to_formatted_s(:db) rescue '-' %>
        </td>
        <td>
          <%= benchmark_execution.end_time.to_formatted_s(:db) rescue '-' %>
        </td>
        <td>
          <%= distance_of_time_in_words(benchmark_execution.duration) rescue '-' %>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
<%= will_paginate @benchmark_executions %>