<div class="box collapsed-box">
  <div class="box-header">
    <h3 class="box-title">
      <%= pluralize(@virtual_machine_instances.count, 'Virtual Machine') %>
    </h3>
    <%= will_paginate @virtual_machine_instances, class: 'pagination-box' %>
    <div class="box-tools pull-right">
      <button class="btn btn-default btn-sm" data-widget="collapse" data-toggle="tooltip" title="" data-original-title="Collapse"><%= fa_icon 'minus' %></button>
    </div>
  </div>
  <div class="box-body table-responsive">
    <%= render 'shared/virtual_machine_instances', show_benchmark_definition: false %>
  </div>
</div>