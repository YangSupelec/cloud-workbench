<%= form_for(@benchmark_definition) do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>

  <%= f.label 'Vagrantfile' %><br>
  <%= text_area_tag :vagrant_file_content, params[:vagrant_file_content], rows: "20", :class => "form-control span9", :id => "autosize" %><br>

  <%= f.submit %>
<% end %>