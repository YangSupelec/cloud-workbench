#!/usr/bin/env sh

# Parse results
<%= @ruby %> -r "./fio_log_parser.rb" -e "FioLogParser.parse_log '<%= @fio_log %>', 'fio_results.csv'"

# Submit metrics
<%= @ruby %> -r "./benchmark_helper.rb" -e "BenchmarkHelper.submit_metrics '<%= @metric_definition_id %>', 'fio_results.csv'"

# Example for submitting a single metric
#<%= @ruby %> -r "./benchmark_helper.rb" -e "BenchmarkHelper.submit_metric '1','122','1313'"

# Notify postprocessing completed
<%= @ruby %> -r "./benchmark_helper.rb" -e "BenchmarkHelper.notify_postprocessing_completed"